generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id          Int      @id @default(autoincrement())
  uid         String   @unique @default(uuid())
  originalUrl String
  shortUrl    String   @unique
  createdAt   DateTime @default(now())
}
